<h1>Admins#index</h1>
<p>Find me in app/views/admins/index.html.erb</p>
<table>
    <tr>
        <th>name</th>
        <th>phone_no</th>
        <th>age</th>
        <th>address</th>
    </tr>
    <% @admin.each do |admin| %>
    <tr>
        <td><%=admin.name %></td>
        <td><%=admin.phone_no %></td>
        <td><%=admin.age %></td>
        <td><%=admin.address %></td>
        <td><%=link_to "Edit",edit_admin_path(admin.id) %></td>
        <td><%=link_to "Delete",admin_path(admin.id), method: :delete%></td>
    </tr>
    <% end %>
</table><br>
<%= link_to "Add Admin",new_admin_path %>
